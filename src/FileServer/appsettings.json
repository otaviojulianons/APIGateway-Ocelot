{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "AllowedHosts": "*",
  "FileServerDirectory": "files",
  "serilog": {
    "console": {
      "enabled": true
    },
    "file": {
      "enabled": true,
      "path": "logs/log-fileserver.txt"
    },    
    "mongodb": {
      "enabled": false,
      "url": "mongodb://localhost:27017/logs"
    },
    "elastic": {
      "enabled": false,
      "url": "http://localhost:9200"
    }
  }
}
